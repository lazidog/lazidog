datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}
generator client {
  provider = "prisma-client-js"
}

model User {
  id                   String                 @id @db.Uuid
  email                String
  createdAt            DateTime               @default(now()) @map("created_at") @db.Timestamptz(6)

  @@map("users")
}

